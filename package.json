{
  "name": "qin.js",
  "version": "0.1.8",
  "description": "A gaming sandbox engine written in JavaScript",
  "main": "bin/Qin.js",
  "scripts": {
    "build": "babel ./src -d bin --copy-files",
    "dev": "node script/devCore.js",
    "doc": "./node_modules/.bin/jsdoc src -c ./docs/jsdoc.json -t ./node_modules/@pixi/jsdoc-template -R README.md -r -d docs",
    "flow": "flow",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "test": "mocha test/**/*.test.js --require @babel/register && npm run lint && npm run build && npm run flow check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TyrealGray/Qin.js.git"
  },
  "author": "TyrealGray",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/TyrealGray/Qin.js/issues"
  },
  "prettier": {
    "useTabs": true,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "parser": "flow"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "homepage": "https://github.com/TyrealGray/Qin.js#readme",
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/register": "7.0.0",
    "@pixi/jsdoc-template": "2.4.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.4",
    "core-js": "2.5.7",
    "eslint": "4.9.0",
    "expect.js": "0.3.1",
    "flow-bin": "0.75.0",
    "jsdoc": "3.5.5",
    "jsdoc-babel": "0.5.0",
    "mocha": "5.1.1",
    "prettier": "1.14.2",
    "webpack": "4.22.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "pouchdb": "7.0.0",
    "pouchdb-find": "7.0.0",
    "redux": "4.0.0",
    "remote-redux-devtools": "0.5.13"
  }
}
