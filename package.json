{
  "name": "qin.js",
  "version": "0.0.1",
  "description": "A story engine written in JavaScript",
  "main": "bin/Qin.js",
  "scripts": {
    "test": "mocha test/**/*.test.js --require babel-core/register && npm run lint && npm run build && npm run flow check",
    "start": "node script/dev.js --require babel-core/register",
    "build": "babel ./src -s -d bin",
    "flow": "flow",
    "jsdoc": "./node_modules/.bin/jsdoc src -c ./docs/jsdoc.json -t ./node_modules/@pixi/jsdoc-template -R README.md -r -d docs",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TyrealGray/Qin.git"
  },
  "author": "TyrealGray",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/TyrealGray/Qin/issues"
  },
  "prettier": {
    "useTabs": true,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "parser": "flow"
  },
  "homepage": "https://github.com/TyrealGray/Qin#readme",
  "devDependencies": {
    "@pixi/jsdoc-template": "2.4.0",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-flow": "6.23.0",
    "babel-register": "6.26.0",
    "eslint": "4.9.0",
    "expect.js": "0.3.1",
    "flow-bin": "0.75.0",
    "jsdoc": "3.5.5",
    "jsdoc-babel": "0.4.0",
    "mocha": "5.1.1",
    "prettier": "1.14.2",
    "remote-redux-devtools": "^0.5.13"
  },
  "dependencies": {
    "big-json": "2.0.2",
    "pixi.js": "4.8.2",
    "pouchdb": "7.0.0",
    "redux": "4.0.0"
  }
}
