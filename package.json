{
  "name": "qin.js",
  "version": "0.1.3",
  "description": "A story engine written in JavaScript",
  "main": "bin/Qin.js",
  "scripts": {
    "test": "mocha test/**/*.test.js --require babel-core/register && npm run lint && npm run build && npm run flow check",
    "start-core": "node script/devForCore.js --require babel-core/register",
    "start": "webpack-dev-server --open --config config/webpack.config.js",
    "build": "babel ./src -d bin --copy-files",
    "flow": "flow",
    "doc": "./node_modules/.bin/jsdoc src -c ./docs/jsdoc.json -t ./node_modules/@pixi/jsdoc-template -R README.md -r -d docs",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TyrealGray/Qin.js.git"
  },
  "author": "TyrealGray",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/TyrealGray/Qin.js/issues"
  },
  "prettier": {
    "useTabs": true,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "parser": "flow"
  },
  "homepage": "https://github.com/TyrealGray/Qin.js#readme",
  "devDependencies": {
    "@pixi/jsdoc-template": "2.4.0",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-flow": "6.23.0",
    "babel-register": "6.26.0",
    "eslint": "4.9.0",
    "expect.js": "0.3.1",
    "flow-bin": "0.75.0",
    "jsdoc": "3.5.5",
    "jsdoc-babel": "0.4.0",
    "mocha": "5.1.1",
    "prettier": "1.14.2",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "pixi.js": "4.8.2",
    "pouchdb": "7.0.0",
    "pouchdb-find": "7.0.0",
    "redux": "4.0.0",
    "remote-redux-devtools": "0.5.13"
  }
}
